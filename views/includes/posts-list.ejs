<div class="home_container">
  <% posts.forEach(post => { %>
    <article class="card">
        <h3 class="card_title"><%= post.title %></h3>
        <p class="card_author">Autor: <strong>Leandro Antunes</strong></p>
        <p class="card_date">Data do post: <span><%= post.created_date %></span></p>
      </article>
  <% }) %>
    </div>